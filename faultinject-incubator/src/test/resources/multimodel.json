{
    "fmus": {
        "{rbmq}": "rabbitmq.fmu",
        "{sMonitor}": "safety_monitor_v3.fmu"
    },
    "connections": {
        "{rbmq}.rbmqInstance.fields.t1": [
            "{sMonitor}.sMonitorInstance.obs_xpos_input",
            "{sMonitor}.sMonitorInstance.obs_ypos_input",
            "{sMonitor}.sMonitorInstance.robotti_xpos_input",
            "{sMonitor}.sMonitorInstance.robotti_ypos_input"
        ],
        "{rbmq}.rbmqInstance.measurement": [
            "{sMonitor}.sMonitorInstance.obstacles_input"
        ],
        "{sMonitor}.sMonitorInstance.command_stop": [
            "{rbmq}.rbmqInstance.command_stop"
        ]
    },
    "parameters": {
        "{rbmq}.rbmqInstance.config.maxage": 60000,
        "{rbmq}.rbmqInstance.config.hostname": "localhost",
        "{rbmq}.rbmqInstance.config.precision": 10,
        "{sMonitor}.sMonitorInstance.safety_dist": 10
    },
    "algorithm":{"type":"fixed-step","size":3}, 
    "faultInjectConfigurationPath": "target/test-classes/faultEvents.xml",
    "faultInjectInstances": {
        "sMonitorInstance": "id"
    }

}